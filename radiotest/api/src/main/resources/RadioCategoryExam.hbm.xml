	<class name="RadioCategoryExam" table="radio_category_exam">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<property name="examFee" type="double" column="exam_fee"
			access="field" />
		<property name="readingFee" type="double" column="reading_fee"
			access="field" />
		
		<many-to-one name="exam" column="exam_id" unique="true"
			class="RadioExam" />
		<many-to-one name="category" column="category_id" unique="true"
			class="RadioCategory" />
	</class>

</hibernate-mapping>
