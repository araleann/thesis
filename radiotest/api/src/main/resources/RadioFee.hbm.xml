	<class name="RadioFee" table="radio_fee">
		<id name="id" type="java.lang.Integer" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<property name="amount" type="double" column="amount"
			access="field" />
			
		<many-to-one name="type" column="type_id" unique="true"
			class="RadioFeeType" />
		<many-to-one name="category" column="ce_id" unique="true"
			class="RadioCategoryExam" />
	</class>

</hibernate-mapping>
